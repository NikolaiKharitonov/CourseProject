<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.accountinggoods">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AccountingGoods">
        <activity
            android:name=".activity.help_about_the_program"
            android:exported="false" />
        <activity
            android:name=".activity.ReportActivity"
            android:exported="true" />
        <activity
            android:name=".activity.SuppliersActivity"
            android:exported="true" />
        <activity
            android:name=".activity.ReturnOrderActivity"
            android:exported="true" />
        <activity
            android:name=".activity.CreateOrderActivity"
            android:exported="true" />
        <activity
            android:name=".activity.OperationsLogActivity"
            android:exported="true" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>